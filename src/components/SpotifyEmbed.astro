---
interface Props {
  albumId: string;
}

const { albumId } = Astro.props;
---

<div>
  <iframe
    style="border-radius:12px"
    src=`https://open.spotify.com/embed/album/${albumId}?utm_source=generator&theme=1`
    width="100%"
    height="152"
    allowfullscreen=""
    allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture"
    loading="lazy"></iframe>
</div>
